# cloudrun/.env.example
# Speckle Configuration
SPECKLE_APP_ID=your_speckle_app_id
SPECKLE_APP_SECRET=your_speckle_app_secret
SPECKLE_SERVER_URL=https://app.speckle.systems

# Firebase Configuration
GOOGLE_APPLICATION_CREDENTIALS=/path/to/firebase-service-account.json

# Session Configuration (auto-generated in production)
SESSION_SECRET_KEY=your_session_secret_key

# Development flags
FLASK_ENV=development
FLASK_DEBUG=1
PORT=8080